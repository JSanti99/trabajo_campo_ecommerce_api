{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "name": "productos",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "brand": {
      "plugin": "users-permissions",
      "model": "user"
    },
    "description": {
      "type": "richtext"
    },
    "rating": {
      "type": "integer",
      "min": 0,
      "max": 5
    },
    "variedades": {
      "collection": "variedad"
    },
    "genero": {
      "type": "enumeration",
      "enum": [
        "H",
        "M"
      ]
    },
    "codigo": {
      "type": "string"
    },
    "categoria": {
      "model": "categorias",
      "via": "productos"
    },
    "slug": {
      "type": "string"
    },
    "image": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "price": {
      "type": "decimal",
      "min": 0
    },
    "hasFreeShipping": {
      "type": "boolean"
    }
  }
}
